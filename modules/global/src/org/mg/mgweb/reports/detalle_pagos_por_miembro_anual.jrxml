	<queryString language="SQL">
		<![CDATA[select t.FECHAHORA as fechahora, t.CONCEPTO, t.ID_EVENTO_TIPO, (t.MONTO_APORTE+t.MONTO_DONACION) as importe, e.* from tbl_transferencias t, tbl_entidades e
