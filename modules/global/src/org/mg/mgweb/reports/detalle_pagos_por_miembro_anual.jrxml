select r.FECHAHORA as fechahora, r.CONCEPTO, r.ID_EVENTO_TIPO, (r.MONTO_APORTE+r.MONTO_DONACION) as importe, e.* from tbl_recibos r, tbl_entidades e
where r.ID_ENTIDAD = e.ID and
