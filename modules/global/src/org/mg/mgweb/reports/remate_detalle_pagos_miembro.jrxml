	<parameter name="id_evento" class="java.lang.Integer"/>
	<parameter name="id_miembro" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select r.descripcion, pagos.*, m.* from tbl_eventos r, 
(SELECT id as nro, fechahora, id_entidad, id_evento, monto_aporte, monto_donacion, 'Transferencia' AS tipo FROM tbl_transferencias 
	UNION ALL 
	SELECT id as nro, fechahora, id_entidad, id_evento, monto_aporte, monto_donacion, 'Efectivo' AS tipo  FROM tbl_recibos r
) as pagos LEFT JOIN  tbl_entidades m ON pagos.id_entidad = m.id
where pagos.id_evento = r.id AND pagos.id_entidad = $P{id_miembro}
ORDER BY pagos.fechahora]]>
	</queryString>
