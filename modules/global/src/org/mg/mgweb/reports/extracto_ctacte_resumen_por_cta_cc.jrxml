    <queryString language="SQL">
		<![CDATA[SELECT ccd.DESCRIPCION AS NOMBRE, -SUM(A.MONTO) AS IMPORTE
                        FROM TBL_FACTURAS_COMPRA f,
                            TBL_FACTURAS_COMPRA_ASIENTOS fa,
                            TBL_ASIENTOS a,
                            TBL_CUENTAS_CONTABLES ccd,
                            TBL_CUENTAS_CONTABLES_POR_DEFECTO ccpd
                        WHERE a.ID_CUENTA_CONTABLE_HABER = ccpd.ID_CUENTA_CTA_CTE
                        AND a.ID_CENTRO_DE_COSTO_HABER = $P{centroDeCosto}
                            AND a.ID_CUENTA_CONTABLE_DEBE = ccd.ID
                            AND ccpd.ID = 1
                            AND a.ID = fa.ID_ASIENTO
                            AND f.ID = fa.ID_FACTURA_COMPRA
                            AND a.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY ccd.DESCRIPCION
                    UNION ALL
                        SELECT cch.DESCRIPCION AS NOMBRE, SUM(a.MONTO) AS IMPORTE
                        FROM TBL_NOTAS_DE_CREDITO_COMPRAS n,
                                TBL_NOTAS_DE_CREDITO_COMPRAS_ASIENTOS na,
                                TBL_ASIENTOS a,
                                TBL_CUENTAS_CONTABLES cch,
                                TBL_CUENTAS_CONTABLES_POR_DEFECTO ccpd
                        WHERE a.ID_CUENTA_CONTABLE_DEBE = ccpd.ID_CUENTA_CTA_CTE
                        AND a.ID_CENTRO_DE_COSTO_DEBE = $P{centroDeCosto}
                            AND a.ID_CUENTA_CONTABLE_HABER = cch.ID
                            AND ccpd.ID = 1
                            AND a.ID = na.ID_ASIENTO
                            AND n.ID = na.ID_NOTA_DE_CREDITO
                            AND a.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY cch.DESCRIPCION
                    UNION ALL
                        SELECT ccd.DESCRIPCION AS NOMBRE, SUM(-a.MONTO) AS IMPORTE
                        FROM TBL_RECIBOS_COMPRA r,
                             TBL_RECIBOS_COMPRA_ASIENTOS ra,
                             TBL_ASIENTOS a,
                             TBL_CUENTAS_CONTABLES ccd,
                             TBL_CUENTAS_CONTABLES_POR_DEFECTO ccpd
                        WHERE a.ID_CUENTA_CONTABLE_HABER = ccpd.ID_CUENTA_CTA_CTE
                        AND a.ID_CENTRO_DE_COSTO_HABER = $P{centroDeCosto}
                            AND a.ID_CUENTA_CONTABLE_HABER = ccd.ID
                            AND ccpd.ID = 1
                            AND a.ID = ra.ID_ASIENTO
                            AND r.ID = ra.ID_RECIBO
                            AND a.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY ccd.DESCRIPCION
                    UNION ALL
                        SELECT ccd.DESCRIPCION AS NOMBRE, SUM(-a.MONTO) AS IMPORTE
                        FROM TBL_AUTOFACTURAS af,
                            TBL_AUTOFACTURAS_ASIENTOS afa,
                            TBL_ASIENTOS a,
                            TBL_CUENTAS_CONTABLES ccd,
                            TBL_CUENTAS_CONTABLES_POR_DEFECTO ccpd
                        WHERE a.ID_CUENTA_CONTABLE_HABER = ccpd.ID_CUENTA_CTA_CTE
                        AND a.ID_CENTRO_DE_COSTO_HABER = $P{centroDeCosto}
                            AND a.ID_CUENTA_CONTABLE_DEBE = ccd.ID
                            AND ccpd.ID = 1
                            AND a.ID = afa.ID_ASIENTO
                            AND af.ID = afa.ID_AUTOFACTURA
                            AND a.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY ccd.DESCRIPCION
                    UNION ALL
                        SELECT ccd.DESCRIPCION AS NOMBRE, SUM(-a.MONTO) AS IMPORTE FROM   TBL_ASIENTOS a,
                            TBL_CUENTAS_CONTABLES ccd,
                            TBL_CUENTAS_CONTABLES cch,
                            TBL_CUENTAS_CONTABLES_POR_DEFECTO ccpd,
                            TBL_CENTROS_DE_COSTO cdc
                        WHERE a.ID_CUENTA_CONTABLE_DEBE = ccd.ID
                            AND a.ID_CUENTA_CONTABLE_HABER = cch.ID
                            AND a.ID_CENTRO_DE_COSTO_DEBE = cdc.ID
                            AND a.ID_CENTRO_DE_COSTO_HABER = $P{centroDeCosto}
                            AND cch.ID = ccpd.ID_CUENTA_CTA_CTE
                            AND ccpd.ID = 1
                            AND a.ASIENTO_MANUAL = TRUE
                            AND a.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY ccd.DESCRIPCION
                    UNION ALL
                        SELECT cch.DESCRIPCION AS NOMBRE, SUM(a.MONTO) AS IMPORTE FROM   TBL_ASIENTOS a,
                            TBL_CUENTAS_CONTABLES ccd,
                            TBL_CUENTAS_CONTABLES cch,
                            TBL_CUENTAS_CONTABLES_POR_DEFECTO ccpd,
                            TBL_CENTROS_DE_COSTO cdc
                        WHERE a.ID_CUENTA_CONTABLE_DEBE = ccd.ID
                            AND a.ID_CUENTA_CONTABLE_HABER = cch.ID
                            AND a.ID_CENTRO_DE_COSTO_HABER = cdc.ID
                            AND a.ID_CENTRO_DE_COSTO_DEBE = $P{centroDeCosto}
                            AND ccd.ID = ccpd.ID_CUENTA_CTA_CTE
                            AND ccpd.ID = 1
                            AND a.ASIENTO_MANUAL = TRUE
                            AND a.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY cch.DESCRIPCION
                    UNION ALL
                        SELECT et.DESCRIPCION as NOMBRE, SUM(aat.MONTO) AS IMPORTE
                        FROM TBL_TRANSFERENCIAS t, TBL_EVENTO_TIPOS et,
                            TBL_TRANSFERENCIAS_ASIENTOS_TEMPORALES tat,
                            TBL_ASIENTOS_TEMPORALES aat
                        WHERE t.COBRADO = TRUE
                        AND t.ID_EVENTO_TIPO = et.ID
                        AND tat.ID_TRANSFERENCIA = t.ID
                        AND tat.ID_ASIENTO_TEMPORAL = aat.ID
                        AND aat.ID_CENTRO_DE_COSTO_DEBE = $P{centroDeCosto}
                        AND aat.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY et.DESCRIPCION
                    UNION ALL
                        SELECT et.DESCRIPCION as NOMBRE, SUM(aat.MONTO) AS IMPORTE
                        FROM TBL_RECIBOS r, TBL_EVENTO_TIPOS et, TBL_RECIBOS_ASIENTOS_TEMPORALES tat, TBL_ASIENTOS_TEMPORALES aat
                        WHERE r.ID_EVENTO_TIPO = et.ID
                        AND tat.ID_RECIBO = r.ID
                        AND tat.ID_ASIENTO_TEMPORAL = aat.ID
                        AND aat.ID_CENTRO_DE_COSTO_HABER = $P{centroDeCosto}
                        AND aat.FECHAHORA BETWEEN $P{fechaDesde} AND $P{fechaHasta}
                            GROUP BY et.DESCRIPCION
                    ]]>
    </queryString>
